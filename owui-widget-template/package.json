{
  "name": "@openwebui-suite/owui-widget-template",
  "version": "0.1.0",
  "description": "Template for creating OpenWebUI frontend widgets",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsc -p tsconfig.json --watch",
    "clean": "rimraf dist",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "openwebui",
    "widget",
    "template",
    "react",
    "typescript"
  ],
  "author": "OpenWebUI Suite",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/<ORG>/owui-widget-template.git"
  },
  "bugs": {
    "url": "https://github.com/<ORG>/owui-widget-template/issues"
  },
  "homepage": "https://github.com/<ORG>/owui-widget-template#readme",
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.45.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "rimraf": "^5.0.0",
    "typescript": "^5.5.0"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  }
}
